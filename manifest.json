{
    "manifest_version": 3,

    "name": "at Anonym",
    "description": "Create an alias of an email to be used on website.",
    "version": "1.0",

    "browser_action": {
     "default_icon": "icons/icon.png",
     "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "host_permissions": [
      "https://api.infomaniak.com/*"
    ],
    "optional_host_permissions":[
        "https://*/*",
        "http://*/*"
    ]
  }
